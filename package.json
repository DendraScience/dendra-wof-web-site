{
  "name": "dendra-wof-web-site",
  "version": "0.0.0",
  "description": "WaterOneFlow Web Site to document and test the WaterOneFlow Web Service.",
  "author": "J. Scott Smith",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "nvm": "source $HOME/.nvm/nvm.sh; nvm use",
    "preview": "vite preview",
    "serve": "vite preview",
    "serve:dist": "http-server --proxy http://localhost:8080? dist",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "ssr:dev": "npm run ssr:server",
    "ssr:prod": "npm run build && npm run ssr:server:prod",
    "ssr:server": "node ./src/server",
    "ssr:server:prod": "cross-env NODE_ENV=production node ./src/server"
  },
  "type": "module",
  "dependencies": {
    "@mdi/js": "^7.1.96",
    "@vueuse/core": "^9.13.0",
    "compression": "^1.7.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "pino": "^8.10.0",
    "pino-pretty": "^9.3.0",
    "qs": "^6.11.0",
    "sirv": "^2.0.2",
    "vite": "^4.1.2",
    "vite-plugin-ssr": "^0.4.85",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.2.47",
    "vuetify": "^3.1.5",
    "xml-beautify": "^1.2.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-vue": "^9.9.0",
    "pino-http": "^8.3.3",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "unplugin-vue-components": "^0.24.0",
    "vite-plugin-eslint": "^1.8.1"
  }
}
